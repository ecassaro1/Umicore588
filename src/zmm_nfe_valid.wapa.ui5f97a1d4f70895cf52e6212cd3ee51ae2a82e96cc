sap.ui.define([                                                                                                                                                                                                                                                
	"sap/ui/core/mvc/Controller",                                                                                                                                                                                                                                 
    "sap/ui/core/UIComponent",                                                                                                                                                                                                                                 
    "sap/ui/core/routing/History"                                                                                                                                                                                                                              
],                                                                                                                                                                                                                                                             
	/**                                                                                                                                                                                                                                                           
	 * @param {typeof sap.ui.core.mvc.Controller} Controller                                                                                                                                                                                                      
	 */                                                                                                                                                                                                                                                           
	function (Controller,UIComponent,History) {                                                                                                                                                                                                                   
		"use strict";                                                                                                                                                                                                                                                
                                                                                                                                                                                                                                                               
		return Controller.extend("umicore.mm.nfevalid.zmmnfevalid.controller.Base", {                                                                                                                                                                                
			onInit: function () {				                                                                                                                                                                                                                                   
                //this.sayHello("Base controller");                                                                                                                                                                                                            
			},                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
			getRouter: function() {                                                                                                                                                                                                                                     
                return UIComponent.getRouterFor(this);                                                                                                                                                                                                         
            },                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
            onNavBack: function() {                                                                                                                                                                                                                            
                var oHistory, sPreviewsHash;                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
                oHistory = History.getInstance();                                                                                                                                                                                                              
                sPreviewsHash = oHistory.getPreviousHash();                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
                if (sPreviewsHash !== undefined) {                                                                                                                                                                                                             
                    window.history.go(-1);                                                                                                                                                                                                                     
                } else {                                                                                                                                                                                                                                       
                    this.getRouter().navTo("TargetList",{},true /*no history*/);                                                                                                                                                                               
                }                                                                                                                                                                                                                                              
            },                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
            sayHello: function(input) {                                                                                                                                                                                                                        
                //console.log(input+" All your base are belong to us!");                                                                                                                                                                                       
            },                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
			hide: function(view,elem) {                                                                                                                                                                                                                                 
				elem.setVisible(false);                                                                                                                                                                                                                                    
				var labels = elem.getLabels();                                                                                                                                                                                                                             
				labels.forEach((each)=>{                                                                                                                                                                                                                                   
					var label = view.byId(each.sId);                                                                                                                                                                                                                          
					label.setVisible(false);                                                                                                                                                                                                                                  
				});				                                                                                                                                                                                                                                                    
			},                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
			disable: function(view,elem) {                                                                                                                                                                                                                              
				elem.setEditable(false);                                                                                                                                                                                                                                   
				var labels = elem.getLabels();                                                                                                                                                                                                                             
				labels.forEach((each)=>{                                                                                                                                                                                                                                   
					var label = view.byId(each.sId);                                                                                                                                                                                                                          
					if (label.setEditable) {                                                                                                                                                                                                                                  
						label.setEditable(false);                                                                                                                                                                                                                                
					}                                                                                                                                                                                                                                                         
				});				                                                                                                                                                                                                                                                    
			},                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
			getGlobalModel: function() {                                                                                                                                                                                                                                
				var globalModel = this.getOwnerComponent().getModel("global");                                                                                                                                                                                             
				return globalModel;                                                                                                                                                                                                                                        
			},                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
			cnpjUnmask: function(cnpj) {                                                                                                                                                                                                                                
				var cleanCnpj = cnpj;                                                                                                                                                                                                                                      
				cleanCnpj = cleanCnpj.replace('.', '');                                                                                                                                                                                                                    
				cleanCnpj = cleanCnpj.replace('.', '');                                                                                                                                                                                                                    
				cleanCnpj = cleanCnpj.replace('/', '');                                                                                                                                                                                                                    
				cleanCnpj = cleanCnpj.replace('-', '');                                                                                                                                                                                                                    
				return cleanCnpj;                                                                                                                                                                                                                                          
			},                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
			getI18nText: function(key) {                                                                                                                                                                                                                                
				return this.getView().getModel("i18n").getResourceBundle().getText(key);                                                                                                                                                                                   
			}                                                                                                                                                                                                                                                           
		});                                                                                                                                                                                                                                                          
	});                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               