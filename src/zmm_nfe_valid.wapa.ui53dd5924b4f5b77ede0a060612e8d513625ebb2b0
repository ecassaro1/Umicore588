sap.ui.define([],	                                                                                                                                                                                                                                             
	function () {                                                                                                                                                                                                                                                 
		"use strict";                                                                                                                                                                                                                                                
                                                                                                                                                                                                                                                               
        function _getDialog(that) {                                                                                                                                                                                                                            
            if (!that._oDivergencesDialog) {                                                                                                                                                                                                                   
                that._oDivergencesDialog = sap.ui.xmlfragment("umicore.mm.nfevalid.zmmnfevalid.view.DivergencesPopup",that);                                                                                                                                   
                that.getView().addDependent(that._oDivergencesDialog);                                                                                                                                                                                         
            }                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
           return that._oDivergencesDialog;                                                                                                                                                                                                                    
        }&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
		var Divergences = {                                                                                                                                                                                                                                          
            popUp: function(that) {                                                                                                                                                                                                                            
                _getDialog(that).open();                                                                                                                                                                                                                       
                                                                                                                                                                                                                                                               
                //clean the selections                                                                                                                                                                                                                         
                var oCadDivList = sap.ui.getCore().byId("cadDivList");                                                                                                                                                                                         
                oCadDivList.removeSelections();                                                                                                                                                                                                                
            },                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
            close: function(that) {                                                                                                                                                                                                                            
                if (that._oDivergencesDialog) {                                                                                                                                                                                                                
                    that._oDivergencesDialog.close();                                                                                                                                                                                                          
                }                                                                                                                                                                                                                                              
            }                                                                                                                                                                                                                                                  
		}                                                                                                                                                                                                                                                            
&nbsp;&nbsp;                                                                                                                                                                                                                                                   
		return Divergences;                                                                                                                                                                                                                                          
	}                                                                                                                                                                                                                                                             
);                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               