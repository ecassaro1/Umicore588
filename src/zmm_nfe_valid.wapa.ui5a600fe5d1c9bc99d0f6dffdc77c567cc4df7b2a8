sap.ui.define([],	                                                                                                                                                                                                                                             
	function () {                                                                                                                                                                                                                                                 
		"use strict";                                                                                                                                                                                                                                                
                                                                                                                                                                                                                                                               
        function _getDialog(that) {                                                                                                                                                                                                                            
            if (!that._oMessageDialog) {                                                                                                                                                                                                                       
                that._oMessageDialog = sap.ui.xmlfragment("umicore.mm.nfevalid.zmmnfevalid.view.MessagePopup",that);                                                                                                                                           
                that.getView().addDependent(that._oMessageDialog);                                                                                                                                                                                             
            }                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
           return that._oMessageDialog;                                                                                                                                                                                                                        
        }&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
		var Message = {                                                                                                                                                                                                                                              
            popUp: function(that) {                                                                                                                                                                                                                            
                _getDialog(that).open();                                                                                                                                                                                                                       
            },                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
            close: function(that) {                                                                                                                                                                                                                            
                that._oMessageDialog.close();                                                                                                                                                                                                                  
            }                                                                                                                                                                                                                                                  
		}                                                                                                                                                                                                                                                            
&nbsp;&nbsp;                                                                                                                                                                                                                                                   
		return Message;                                                                                                                                                                                                                                              
	}                                                                                                                                                                                                                                                             
);                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               