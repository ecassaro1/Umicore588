<core:FragmentDefinition                                                                                                                                                                                                                                       
   xmlns="sap.m"                                                                                                                                                                                                                                               
   xmlns:core="sap.ui.core" >                                                                                                                                                                                                                                  
   <Dialog                                                                                                                                                                                                                                                     
      title="{i18n>Divergences}"                                                                                                                                                                                                                               
      contentWidth="750px"                                                                                                                                                                                                                                     
   >                                                                                                                                                                                                                                                           
      <List                                                                                                                                                                                                                                                    
        id="cadDivListShow"                                                                                                                                                                                                                                    
        items="{to_Cods}">                                                                                                                                                                                                                                     
        <StandardListItem                                                                                                                                                                                                                                      
          title="{CodDiv}"                                                                                                                                                                                                                                     
          description="{NomeDiv}"/>                                                                                                                                                                                                                            
      </List>&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
      <beginButton>                                                                                                                                                                                                                                            
        <Button&nbsp;                                                                                                                                                                                                                                          
          id="cadDivShow_ok"                                                                                                                                                                                                                                   
          text="Ok"                                                                                                                                                                                                                                            
          press="onOkDivergencesShowPopup"/>&nbsp;&nbsp;                                                                                                                                                                                                       
      </beginButton>                                                                                                                                                                                                                                           
   </Dialog>                                                                                                                                                                                                                                                   
</core:FragmentDefinition>                                                                                                                                                                                                                                     