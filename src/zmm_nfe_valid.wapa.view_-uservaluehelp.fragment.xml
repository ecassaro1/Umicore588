<core:FragmentDefinition                                                                                                                                                                                                                                       
	xmlns="sap.ui.comp.valuehelpdialog"                                                                                                                                                                                                                           
	xmlns:core="sap.ui.core"                                                                                                                                                                                                                                      
    xmlns:m="sap.m"                                                                                                                                                                                                                                            
    xmlns:fb="sap.ui.comp.filterbar">                                                                                                                                                                                                                          
	<ValueHelpDialog                                                                                                                                                                                                                                              
		title="{i18n>User}"                                                                                                                                                                                                                                          
		ok=".onUserValueHelpOkPress"                                                                                                                                                                                                                                 
		cancel=".onUserValueHelpCancelPress"                                                                                                                                                                                                                         
		afterClose=".onUserValueHelpAfterClose"                                                                                                                                                                                                                      
		supportMultiselect="false"                                                                                                                                                                                                                                   
		key="BName"                                                                                                                                                                                                                                                  
		descriptionKey="NameText"                                                                                                                                                                                                                                    
	>                                                                                                                                                                                                                                                             
		<filterBar>                                                                                                                                                                                                                                                  
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch" isRunningInValueHelpDialog="true">                                                                                                                                                            
                                                                                                                                                                                                                                                               
			</fb:FilterBar>                                                                                                                                                                                                                                             
		</filterBar>                                                                                                                                                                                                                                                 
	</ValueHelpDialog>                                                                                                                                                                                                                                            
</core:FragmentDefinition>                                                                                                                                                                                                                                     