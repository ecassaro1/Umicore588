<core:FragmentDefinition                                                                                                                                                                                                                                       
   xmlns="sap.m"                                                                                                                                                                                                                                               
   xmlns:core="sap.ui.core" >                                                                                                                                                                                                                                  
   <Dialog                                                                                                                                                                                                                                                     
      title="{i18n>Divergences}"                                                                                                                                                                                                                               
      contentWidth="750px"                                                                                                                                                                                                                                     
   >                                                                                                                                                                                                                                                           
      <List                                                                                                                                                                                                                                                    
        id="cadDivList"                                                                                                                                                                                                                                        
        items="{masterData>/CadDiv}"                                                                                                                                                                                                                           
        mode="MultiSelect"                                                                                                                                                                                                                                     
        includeItemInSelection="true">                                                                                                                                                                                                                         
        <StandardListItem                                                                                                                                                                                                                                      
          title="{masterData>CodDiv}"                                                                                                                                                                                                                          
          description="{masterData>NomeDiv}"/>                                                                                                                                                                                                                 
      </List>&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
      <beginButton>                                                                                                                                                                                                                                            
        <Button&nbsp;                                                                                                                                                                                                                                          
          id="cadDiv_ok"                                                                                                                                                                                                                                       
          text="Ok"                                                                                                                                                                                                                                            
          press="onOkDivergencesPopup"/>&nbsp;&nbsp;                                                                                                                                                                                                           
      </beginButton>                                                                                                                                                                                                                                           
      <endButton>                                                                                                                                                                                                                                              
        <Button&nbsp;                                                                                                                                                                                                                                          
          id="cadDiv_cancel"                                                                                                                                                                                                                                   
          text="{i18n>Cancel}"                                                                                                                                                                                                                                 
          press="onCancDivergencesPopup"/>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                 
      </endButton>                                                                                                                                                                                                                                             
   </Dialog>                                                                                                                                                                                                                                                   
</core:FragmentDefinition>                                                                                                                                                                                                                                     