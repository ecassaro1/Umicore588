sap.ui.define([], function () {                                                                                                                                                                                                                                
	"use strict";                                                                                                                                                                                                                                                 
	return {                                                                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
		alpha: function(sString){                                                                                                                                                                                                                                    
			let s = "000000" + sString;                                                                                                                                                                                                                                 
			return s.substring(s.length-6);                                                                                                                                                                                                                             
		},                                                                                                                                                                                                                                                           
		cnpj: function (cnpj) {                                                                                                                                                                                                                                      
			if (!cnpj) return;                                                                                                                                                                                                                                          
			return cnpj.replace(/(\d{2})(\d{3})(\d{3})(\d{4})(\d{2})/g,"\$1.\$2.\$3\/\$4\-\$5");                                                                                                                                                                        
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		validStatusText: function(status) {                                                                                                                                                                                                                          
			switch(status) {                                                                                                                                                                                                                                            
				case "S": return this.getI18nText("ValidStatOk");&nbsp;                                                                                                                                                                                                    
				case "E": return this.getI18nText("ValidStatError");                                                                                                                                                                                                       
				case "W": return this.getI18nText("ValidStatWarning");                                                                                                                                                                                                     
				case "C": return this.getI18nText("ValidStatCanceled");                                                                                                                                                                                                    
				default: return this.getI18nText("ValidStatToBeValidated");                                                                                                                                                                                                
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		validStatusIcon: function(status) {                                                                                                                                                                                                                          
			switch(status) {                                                                                                                                                                                                                                            
				case "S": return "success";                                                                                                                                                                                                                                
				case "E": return "error";                                                                                                                                                                                                                                  
				case "W": return "alert";                                                                                                                                                                                                                                  
				case "C": return "information";                                                                                                                                                                                                                            
				default: return "to-be-reviewed"                                                                                                                                                                                                                           
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		validStatusState: function(status) {                                                                                                                                                                                                                         
			switch(status) {                                                                                                                                                                                                                                            
				case "S": return "Success";                                                                                                                                                                                                                                
				case "E": return "Error";                                                                                                                                                                                                                                  
				case "W": return "Warning";                                                                                                                                                                                                                                
				case "C": return "Information";                                                                                                                                                                                                                            
				default: return "None";                                                                                                                                                                                                                                    
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		ctrlStatusText: function(status) {                                                                                                                                                                                                                           
			switch(status) {                                                                                                                                                                                                                                            
				case "0": return this.getI18nText("CtrlStatUndefined");                                                                                                                                                                                                    
				case "1": return this.getI18nText("CtrlStatReleased");                                                                                                                                                                                                     
				case "2": return this.getI18nText("CtrlStatArchived");                                                                                                                                                                                                     
				case "3": return this.getI18nText("CtrlStatUndone");                                                                                                                                                                                                       
				case "8": return this.getI18nText("CtrlStatNotProcessed");                                                                                                                                                                                                 
				case "9": return this.getI18nText("CtrlStatRejected");                                                                                                                                                                                                     
				default: return this.getI18nText("CtrlStatUndefined");                                                                                                                                                                                                     
			}                                                                                                                                                                                                                                                           
		},		                                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
		ctrlStatusIcon: function(status) {                                                                                                                                                                                                                           
			switch(status) {                                                                                                                                                                                                                                            
				case "0": return "to-be-reviewed";                                                                                                                                                                                                                         
				case "1": return "sys-enter-2";                                                                                                                                                                                                                            
				case "2": return "information";                                                                                                                                                                                                                            
				case "3": return "information";                                                                                                                                                                                                                            
				case "9": return "error";                                                                                                                                                                                                                                  
				default: return "to-be-reviewed";				                                                                                                                                                                                                                      
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		ctrlStatusState: function(status) {                                                                                                                                                                                                                          
			switch(status) {                                                                                                                                                                                                                                            
				case "0": return "None";                                                                                                                                                                                                                                   
				case "1": return "Success";                                                                                                                                                                                                                                
				case "2": return "Information";                                                                                                                                                                                                                            
				case "3": return "Information";                                                                                                                                                                                                                            
				case "9": return "Error";                                                                                                                                                                                                                                  
				default: return "None";					                                                                                                                                                                                                                               
			}			                                                                                                                                                                                                                                                        
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		actionRequired: function(actReq) {                                                                                                                                                                                                                           
			switch(actReq) {                                                                                                                                                                                                                                            
				case "0": return this.getI18nText("ARNone");                                                                                                                                                                                                               
				case "1": return this.getI18nText("ARGoodsRecepient");                                                                                                                                                                                                     
				case "2": return this.getI18nText("ARSupplies");                                                                                                                                                                                                           
				case "3": return this.getI18nText("ARVendor");                                                                                                                                                                                                             
				case "4": return this.getI18nText("ARFiscal");                                                                                                                                                                                                             
				case "5": return this.getI18nText("ARSuppliesVendor");                                                                                                                                                                                                     
				case "6": return this.getI18nText("ARConcluded");                                                                                                                                                                                                          
				case "7": return this.getI18nText("ARReadyToAcc");                                                                                                                                                                                                         
				default: return this.getI18nText("ARNone");                                                                                                                                                                                                                
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		finality: function(fin) {                                                                                                                                                                                                                                    
			switch(fin) {                                                                                                                                                                                                                                               
				case "1": return this.getI18nText("FinNormal");                                                                                                                                                                                                            
				case "2": return this.getI18nText("FinComplementary");                                                                                                                                                                                                     
				case "3": return this.getI18nText("FinAdjustment");                                                                                                                                                                                                        
				case "4": return this.getI18nText("FinGoodsReturn");                                                                                                                                                                                                       
				default: return this.getI18nText("FinNone");                                                                                                                                                                                                               
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		pin: function(pin) {                                                                                                                                                                                                                                         
			switch(pin) {                                                                                                                                                                                                                                               
				case "":                                                                                                                                                                                                                                                   
				case "0": return this.getI18nText("Undefined");				                                                                                                                                                                                                        
				case "1": return this.getI18nText("NotRequired");				                                                                                                                                                                                                      
				case "2": return this.getI18nText("Analyze");                                                                                                                                                                                                              
				case "3": return this.getI18nText("Required");                                                                                                                                                                                                             
				case "4": return this.getI18nText("Accepted");                                                                                                                                                                                                             
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		tpnf: function(tp) {                                                                                                                                                                                                                                         
			switch (tp) {                                                                                                                                                                                                                                               
				case "0": return this.getI18nText("TpNfInbound");                                                                                                                                                                                                          
				case "1": return this.getI18nText("TpNfOutbound");                                                                                                                                                                                                         
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		finality: function(fin) {                                                                                                                                                                                                                                    
			switch (fin) {                                                                                                                                                                                                                                              
				case "1": return  this.getI18nText("Finality1");                                                                                                                                                                                                           
				case "2": return  this.getI18nText("Finality2");                                                                                                                                                                                                           
				case "3": return  this.getI18nText("Finality3");                                                                                                                                                                                                           
				case "4": return  this.getI18nText("Finality4");                                                                                                                                                                                                           
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		modFrete: function(mf) {                                                                                                                                                                                                                                     
			switch (mf) {                                                                                                                                                                                                                                               
				case "0": return  this.getI18nText("mf0");                                                                                                                                                                                                                 
				case "1": return  this.getI18nText("mf1");                                                                                                                                                                                                                 
				case "2": return  this.getI18nText("mf2");                                                                                                                                                                                                                 
				case "3": return  this.getI18nText("mf3");                                                                                                                                                                                                                 
				case "4": return  this.getI18nText("mf4");                                                                                                                                                                                                                 
				case "9": return  this.getI18nText("mf9");				                                                                                                                                                                                                             
			}		                                                                                                                                                                                                                                                         
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		procstat: function(ps) {                                                                                                                                                                                                                                     
			switch(ps) {                                                                                                                                                                                                                                                
				case "01": return this.getI18nText("POProcStat01");                                                                                                                                                                                                        
				case "02": return this.getI18nText("POProcStat02");                                                                                                                                                                                                        
				case "03": return this.getI18nText("POProcStat03");                                                                                                                                                                                                        
				case "04": return this.getI18nText("POProcStat04");                                                                                                                                                                                                        
				case "05": return this.getI18nText("POProcStat05");                                                                                                                                                                                                        
				case "08": return this.getI18nText("POProcStat08");                                                                                                                                                                                                        
				case "11": return this.getI18nText("POProcStat11");                                                                                                                                                                                                        
				case "12": return this.getI18nText("POProcStat12");                                                                                                                                                                                                        
				case "13": return this.getI18nText("POProcStat13");                                                                                                                                                                                                        
				case "14": return this.getI18nText("POProcStat14");                                                                                                                                                                                                        
				case "26": return this.getI18nText("POProcStat26");                                                                                                                                                                                                        
				default : return ps;                                                                                                                                                                                                                                       
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		nfWriteMsgIcon: function(type) {                                                                                                                                                                                                                             
			switch(type) {                                                                                                                                                                                                                                              
				case "E": return "error";                                                                                                                                                                                                                                  
				case "I": return "information";                                                                                                                                                                                                                            
				case "W": return "information";                                                                                                                                                                                                                            
				default: return "error";				                                                                                                                                                                                                                               
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		nfWriteMsgTitle: function(type) {                                                                                                                                                                                                                            
			switch(type) {                                                                                                                                                                                                                                              
				case "E": return this.getI18nText("Error");                                                                                                                                                                                                                
				case "I": return this.getI18nText("Information");                                                                                                                                                                                                          
				case "W": return this.getI18nText("Information");                                                                                                                                                                                                          
				default: return this.getI18nText("Error");				                                                                                                                                                                                                             
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
	};                                                                                                                                                                                                                                                            
});                                                                                                                                                                                                                                                            