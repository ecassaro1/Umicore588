<core:FragmentDefinition                                                                                                                                                                                                                                       
   xmlns="sap.m"                                                                                                                                                                                                                                               
   xmlns:core="sap.ui.core" >                                                                                                                                                                                                                                  
   <Dialog                                                                                                                                                                                                                                                     
      title="{i18n>NfWriteMsgs}"                                                                                                                                                                                                                               
      contentWidth="750px"                                                                                                                                                                                                                                     
   >                                                                                                                                                                                                                                                           
      <List                                                                                                                                                                                                                                                    
        id="nfWriteMsgsList"                                                                                                                                                                                                                                   
        items="{global>/NfWriteMsgs}">                                                                                                                                                                                                                         
        <StandardListItem                                                                                                                                                                                                                                      
            title="{                                                                                                                                                                                                                                           
               path: 'global>type',                                                                                                                                                                                                                            
               formatter: '.formatter.nfWriteMsgTitle'                                                                                                                                                                                                         
            }"                                                                                                                                                                                                                                                 
            icon="sap-icon://{                                                                                                                                                                                                                                 
               path: 'global>type',                                                                                                                                                                                                                            
               formatter: '.formatter.nfWriteMsgIcon'                                                                                                                                                                                                          
            }"                                                                                                                                                                                                                                                 
            description="{global>message}"                                                                                                                                                                                                                     
          />                                                                                                                                                                                                                                                   
      </List>&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
      <endButton>                                                                                                                                                                                                                                              
        <Button&nbsp;                                                                                                                                                                                                                                          
          id="nfWriteMsg_ok"                                                                                                                                                                                                                                   
          text="Ok"                                                                                                                                                                                                                                            
          press="onOkNfWriteMsgsPopup"/>&nbsp;&nbsp;                                                                                                                                                                                                           
      </endButton>                                                                                                                                                                                                                                             
   </Dialog>                                                                                                                                                                                                                                                   
</core:FragmentDefinition>                                                                                                                                                                                                                                     