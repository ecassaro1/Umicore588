sap.ui.define([],	                                                                                                                                                                                                                                             
	function () {                                                                                                                                                                                                                                                 
		"use strict";                                                                                                                                                                                                                                                
                                                                                                                                                                                                                                                               
        function _getDialog(that) {                                                                                                                                                                                                                            
            if (!that._oDivergencesShowDialog) {                                                                                                                                                                                                               
                that._oDivergencesShowDialog = sap.ui.xmlfragment("umicore.mm.nfevalid.zmmnfevalid.view.DivergencesShowPopup",that);                                                                                                                           
                that.getView().addDependent(that._oDivergencesShowDialog);                                                                                                                                                                                     
            }                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
           return that._oDivergencesShowDialog;                                                                                                                                                                                                                
        }&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
		var Divergences = {                                                                                                                                                                                                                                          
            popUp: function(that) {                                                                                                                                                                                                                            
                _getDialog(that).open();                                                                                                                                                                                                                       
                                                                                                                                                                                                                                                               
            },                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
            close: function(that) {                                                                                                                                                                                                                            
                if (that._oDivergencesShowDialog) {                                                                                                                                                                                                            
                    that._oDivergencesShowDialog.close();                                                                                                                                                                                                      
                }                                                                                                                                                                                                                                              
            }                                                                                                                                                                                                                                                  
		}                                                                                                                                                                                                                                                            
&nbsp;&nbsp;                                                                                                                                                                                                                                                   
		return Divergences;                                                                                                                                                                                                                                          
	}                                                                                                                                                                                                                                                             
);                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               