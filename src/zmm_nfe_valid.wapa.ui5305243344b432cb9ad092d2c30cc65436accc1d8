                            name : "Estado",                                                                                                                                                                                                                   
                            template : {                                                                                                                                                                                                                       
                                content : {                                                                                                                                                                                                                    
                                    parts : ["to_eDocIt/POProcstat"],                                                                                                                                                                                          
                                    formatter : function(state) {                                                                                                                                                                                              
                                        if (!state) return;                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
                                        switch (state) {                                                                                                                                                                                                       
                                            case "01": return "01-Versão em processo";                                                                                                                                                                         
                                            case "02": return "02-Ativo";                                                                                                                                                                                      
                                            case "03": return "03-Em liberação";                                                                                                                                                                               
                                            case "04": return "04-Liberação parcial";                                                                                                                                                                          
                                            case "05": return "05-Liberação concluída";                                                                                                                                                                        
                                            case "08": return "08-Recusado";                                                                                                                                                                                   
                                            case "11": return "11-Na distribuição";                                                                                                                                                                            
                                            case "12": return "12-Erro na distribuição";                                                                                                                                                                       
                                            case "13": return "13-Distribuído";                                                                                                                                                                                
                                            case "14": return "14-Em preparação";                                                                                                                                                                              
                                            case "26": return "26-Em aprovação externa";                                                                                                                                                                       
                                            default : return state;                                                                                                                                                                                            
                                        }                                                                                                                                                                                                                      
                                    }&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;        
                                }	                                                                                                                                                                                                                             
                            }                                                                                                                                                                                                                                  
